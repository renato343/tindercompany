<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="hibernateCadetDao" class="org.renato.model.dao.hibernate.HibernateCadetDao">
        <property name="hibernateSessionManager" ref="hibernateSessionManager"/>
    </bean>

    <bean id="hibernateCompanyDao" class="org.renato.model.dao.hibernate.HibernateCompanyDao">
        <property name="hibernateSessionManager" ref="hibernateSessionManager"/>
    </bean>


</beans>

